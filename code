import pandas as pd

# Load the data from the CSV file
file = 
filtered_data = pd.read_csv(file)

# Display the column namesprint(filtered_data.columns)

# Calculate the variation for the 'BTC price' column
filtered_data['BTC Variation'] = filtered_data['BTC price'].diff()

# Calculate the percentage variation of Bitcoin
filtered_data['% Variation BTC'] = (filtered_data['BTC Variation'] / filtered_data['BTC price'].shift(1)) * 100

# Calculate the variation for ''
filtered_data['M2 Variation'] = filtered_data['M2 Growth YoY (%) décalé de  jours'].diff()'


# Rearranging columns in the specified order
filtered_data = filtered_data[['DateTime', 
                                

# Forcer l'affichage de toutes les données
pd.set_option('display.max_rows', None)     # Afficher toutes les lignes
pd.set_option('display.max_columns', None)  # Afficher toutes les colonnes
pd.set_option('display.width', 1000)        # Éviter que les colonnes soient coupées
pd.set_option('display.float_format', '{:.6f}'.format)  # Afficher 6 décimales pour éviter la notation scientifique

# Affichage du DataFrame complet
print(filtered_data)
